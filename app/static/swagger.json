                    type: string
  /exercise/edit:
    put:
      tags:
        - exercise
      summary: Edit exercise
      parameters:
        - name: exercise_ID
          in: query
          description: Exercise ID
          required: true
          schema:
            type: string
        - name: id_muscle
          in: query
          description: Muscle ID
          required: true
          schema:
            type: integer
        - name: name
          in: query
          description: Exercise name
          required: true
          schema:
            type: string
        - name: description
          in: query
          description: Exercise description
          required: true
          schema:
