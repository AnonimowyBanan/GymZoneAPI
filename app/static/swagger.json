openapi: 3.0.0
info:
  title: GymZoneAPI
  description: API for GymZone application
  version: 1.0.0
tags:
  - name: exercise
    description: Everything about your Pets
    externalDocs:
      description: Find out more
      url: http://swagger.io
  - name: user
    description: Everything about your Pets
    externalDocs:
      description: Find out more
      url: http://swagger.io
  - name: biometric
    description: Everything about your Pets
    externalDocs:
      description: Find out more
      url: http://swagger.io
paths:
  /exercise/get-all:
    get:
      tags:
        - exercise
      summary: Get all exercises
      responses:
        '200':
          description: Successful response
          content:
            application/json:
              schema:
                type: array
                items:
                  type: object
                  properties:
                    exercise:
                      type: object
                      properties:
                        id:
                          type: integer
                        id_muscle:
                          type: integer
                        name:
                          type: string
                        description:
                          type: string
                    muscle:
                      type: object
                      properties:
                        id:
                          type: integer
                        name:
                          type: string
                        picture:
                          type: string
  /exercise/get:
    get:
      tags:
        - exercise
      summary: Get exercise by ID
      parameters:
        - name: exercise_ID
          in: query
          description: Exercise ID
          required: true
          schema:
            type: string
      responses:
        '200':
          description: Successful response
          content:
            application/json:
              schema:
                type: object
                properties:
                  exercise:
                    type: object
                    properties:
                      id:
                        type: integer
                      id_muscle:
                        type: integer
                      name:
                        type: string
                      description:
                        type: string
                  muscle:
                    type: object
                    properties:
                      id:
                        type: integer
                      name:
                        type: string
                      picture:
                        type: string
  /exercise/delete:
    delete:
      tags:
        - exercise
      summary: Delete exercise
      parameters:
        - name: exercise_ID
          in: query
          description: Exercise ID
          required: true
          schema:
            type: string
      responses:
        '200':
