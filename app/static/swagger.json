            application/json:
              schema:
                type: array
                items:
                  type: object
                  properties:
                    id:
                      type: integer
                    email:
                      type: string
                    login:
                      type: string
                    password:
                      type: string
                    first_name:
                      type: string
                    last_name:
                      type: string
                    sex:
                      type: string
                    user_picture_file_name:
                      type: string
                    user_description:
                      type: string
                    birthday:
                      type: string
                    register_timestamp:
                      type: string
                      format: date-time
        '204':
          description: No users found
        '500':
          description: Internal server error
  /user/get:
